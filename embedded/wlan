	Unnamed/non-netdev interface
		wdev 0x2
		addr ba:27:eb:18:80:cb
		type P2P-device
		txpower 31.00 dBm
	Interface wlan0
		ifindex 3
		wdev 0x1
		addr b8:27:eb:18:80:cb
		type managed
		channel 165 (5825 MHz), width: 20 MHz, center1: 5825 MHz
		txpower 31.00 dBm
